{
  "name": "FantasyWorldMapGenerator",
  "version": "0.0.1",
  "description": "A map generator for fantasy world maps.",
  "main": "game.js",
  "scripts": {
    "start": "webpack serve --config webpack/webpack.dev.js",
    "build": "rimraf dist && webpack --config webpack/webpack.prod.js",
    "bundle": "npm run build",
    "serve": "serve dist"
  },
  "keywords": [
    "Phaser",
    "Phaser 3",
    "Phaser3",
    "map",
    "d3-delaunay",
    "fantasy map",
    "procedural"
  ],
  "author": {
    "name": "David Diener"
  },
  "license": "MIT",
  "dependencies": {
    "d3-delaunay": "^6.0.4",
    "phaser": "^3.55.2",
    "seedrandom": "^3.0.5",
    "simplex-noise": "^4.0.1"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^10.1.0",
    "html-webpack-plugin": "^5.5.0",
    "javascript-obfuscator": "^4.0.0",
    "rimraf": "^3.0.2",
    "serve": "^14.1.2",
    "ts-loader": "^9.2.6",
    "typescript": "^4.5.3",
    "webpack": "^5.65.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.6.0",
    "webpack-merge": "^5.8.0",
    "webpack-obfuscator": "^3.5.0",
    "workbox-webpack-plugin": "^6.4.2"
  }
